@model IEnumerable<Proyecto2.Models.TarjetaDebito>

@{
    ViewBag.Title = "TajetasUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>TajetasUser</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CuentaMonetaria.NombreCuenta)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumeroTarjeta)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Credito)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CuentaMonetariaSaldo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pin)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CuentaMonetaria.NombreCuenta)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumeroTarjeta)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Credito)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CuentaMonetariaSaldo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pin)
        </td>
    </tr>
}

</table>
